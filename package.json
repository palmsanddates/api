{
	"name": "backend",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"clean": "rm -rf ./dist && mkdir dist",
		"build": "npm run clean && babel ./src --out-dir dist",
		"start-dev": "npm run build && nodemon --exec babel-node ./src/server.js",
		"start-prod": "npm run build && node ./dist/server.js",
		"start-test": "mocha tests/*.js",
		"prepare": "husky install",
		"lint": "pretty-quick --staged && lint-staged"
	},
	"dependencies": {
		"aws-sdk": "^2.1005.0",
		"bcryptjs": "^2.4.3",
		"cors": "^2.8.5",
		"dotenv": "^10.0.0",
		"express": "~4.16.1",
		"express-validator": "^6.12.2",
		"helmet": "^4.6.0",
		"http-errors": "~1.6.3",
		"jsonwebtoken": "^8.5.1",
		"mongoose": "^6.0.6",
		"morgan": "~1.9.1"
	},
	"main": "./src/server.js",
	"keywords": [],
	"author": "Yin",
	"license": "MIT",
	"description": "",
	"devDependencies": {
		"@babel/cli": "^7.15.7",
		"@babel/core": "^7.15.5",
		"@babel/node": "^7.15.4",
		"@babel/preset-env": "^7.15.6",
		"chai": "^4.3.4",
		"chai-http": "^4.3.0",
		"dotenv-safe": "^8.2.0",
		"eslint": "^7.32.0",
		"eslint-config-airbnb-base": "^14.2.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.25.2",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^7.0.4",
		"lint-staged": "^11.2.4",
		"mocha": "^9.1.2",
		"nodemon": "^2.0.12",
		"prettier": "^2.4.1",
		"pretty-quick": "^3.1.1"
	},
	"lint-staged": {
		"*.js": "eslint --cache --fix"
	}
}
